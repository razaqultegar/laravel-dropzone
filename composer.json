{
    "name": "razaqultegar/laravel-dropzone",
    "description": "Reusable Dropzone handler for Laravel with chunk upload support.",
    "keywords": [
        "laravel-dropzone",
        "dropzone-laravel",
        "dropzone"
    ],
    "homepage": "https://github.com/razaqultegar/laravel-dropzone",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Razaqul Tegar",
            "email": "myrazaqul@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "RazaqulTegar\\Dropzone\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "RazaqulTegar\\Dropzone\\Providers\\DropzoneServiceProvider"
            ],
            "aliases": {
                "Dropzone": "RazaqulTegar\\Dropzone\\Facades\\Dropzone"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
